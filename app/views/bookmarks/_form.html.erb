<%= simple_form_for(@bookmark) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :BOOK_WORK %>
    <%= f.input :BOOK_WINDOW %>
    <%= f.input :BOOK_FIND_VALUE %>
    <%= f.input :BOOK_DESC %>
    <%= f.input :BOOK_FIND_FIELD %>
    <%= f.input :BOOK_SORT %>
    <%= f.input :BOOK_DESC_USER %>
    <%= f.input :BOOK_DO_METHOD %>
    <%= f.input :BOOK_DO_CALC %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
