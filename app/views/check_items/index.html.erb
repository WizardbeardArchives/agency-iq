<h1>Listing check_items</h1>

<table>
  <thead>
    <tr>
      <th>Primarykey</th>
      <th>Cx ck date</th>
      <th>Cx net amt</th>
      <th>Cx dgl</th>
      <th>Cx ap num</th>
      <th>Cx vendor num</th>
      <th>Cx job num</th>
      <th>Cx task</th>
      <th>Cx posted</th>
      <th>Cx gross amt</th>
      <th>Cx ck num</th>
      <th>Cx comm amt</th>
      <th>Cx markup pc</th>
      <th>Cx disc amt</th>
      <th>Cx period</th>
      <th>Cx cost  seq</th>
      <th>Cx disc dgl</th>
      <th>Cx billable</th>
      <th>Cx note</th>
      <th>Cx added by</th>
      <th>Cx added when</th>
      <th>Cx edited by</th>
      <th>Cx edited when</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @check_items.each do |check_item| %>
      <tr>
        <td><%= check_item.primaryKey %></td>
        <td><%= check_item.CX_CK_DATE %></td>
        <td><%= check_item.CX_NET_AMT %></td>
        <td><%= check_item.CX_DGL %></td>
        <td><%= check_item.CX_AP_NUM %></td>
        <td><%= check_item.CX_VENDOR_NUM %></td>
        <td><%= check_item.CX_JOB_NUM %></td>
        <td><%= check_item.CX_TASK %></td>
        <td><%= check_item.CX_POSTED %></td>
        <td><%= check_item.CX_GROSS_AMT %></td>
        <td><%= check_item.CX_CK_NUM %></td>
        <td><%= check_item.CX_COMM_AMT %></td>
        <td><%= check_item.CX_MARKUP_PC %></td>
        <td><%= check_item.CX_DISC_AMT %></td>
        <td><%= check_item.CX_PERIOD %></td>
        <td><%= check_item.CX_COST__SEQ %></td>
        <td><%= check_item.CX_DISC_DGL %></td>
        <td><%= check_item.CX_BILLABLE %></td>
        <td><%= check_item.CX_NOTE %></td>
        <td><%= check_item.CX_ADDED_BY %></td>
        <td><%= check_item.CX_ADDED_WHEN %></td>
        <td><%= check_item.CX_EDITED_BY %></td>
        <td><%= check_item.CX_EDITED_WHEN %></td>
        <td><%= link_to 'Show', check_item %></td>
        <td><%= link_to 'Edit', edit_check_item_path(check_item) %></td>
        <td><%= link_to 'Destroy', check_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Check item', new_check_item_path %>
