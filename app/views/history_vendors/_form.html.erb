<%= simple_form_for(@history_vendor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :HV_VENDOR_NUM %>
    <%= f.input :HV_NUM %>
    <%= f.input :HV_ADDED_BY %>
    <%= f.input :HV_DATE %>
    <%= f.input :HV_SUBJECT %>
    <%= f.input :HV_OK %>
    <%= f.input :HV_FOR %>
    <%= f.input :HV_DESC %>
    <%= f.input :HV_MAIL_TO %>
    <%= f.input :HV_CC %>
    <%= f.input :HV_BCC %>
    <%= f.input :HV_NOTIFIED %>
    <%= f.input :HV_INV_NUM %>
    <%= f.input :HV_EDITED_BY %>
    <%= f.input :HV_EDITED_WHEN %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
