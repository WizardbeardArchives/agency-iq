<%= simple_form_for(@assets_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :AST_FILE_EXT %>
    <%= f.input :AST_APP_NAME %>
    <%= f.input :AST_DESC %>
    <%= f.input :AST_CREATOR %>
    <%= f.input :AST_ICON %>
    <%= f.input :AST_USER %>
    <%= f.input :AST_KIND %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
