<%= simple_form_for(@web_ip_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :WIP_IP %>
    <%= f.input :WIP_NAME %>
    <%= f.input :WIP_DATE %>
    <%= f.input :WIP_DATETIME %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
