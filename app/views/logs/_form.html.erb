<%= simple_form_for(@log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :LOG_DATE %>
    <%= f.input :LOG_TIME %>
    <%= f.input :LOG_USER %>
    <%= f.input :LOG_NOTES %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
