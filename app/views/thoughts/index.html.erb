<h1>Listing thoughts</h1>

<table>
  <thead>
    <tr>
      <th>Primarykey</th>
      <th>Msg date</th>
      <th>Msg text</th>
      <th>Msg 1</th>
      <th>Msg daily info ctr</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @thoughts.each do |thought| %>
      <tr>
        <td><%= thought.primaryKey %></td>
        <td><%= thought.MSG_DATE %></td>
        <td><%= thought.MSG_TEXT %></td>
        <td><%= thought.MSG_1 %></td>
        <td><%= thought.MSG_DAILY_INFO_CTR %></td>
        <td><%= link_to 'Show', thought %></td>
        <td><%= link_to 'Edit', edit_thought_path(thought) %></td>
        <td><%= link_to 'Destroy', thought, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thought', new_thought_path %>
