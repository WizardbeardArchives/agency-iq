<%= simple_form_for(@rate) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :RATE_STAFF %>
    <%= f.input :RATE_CLIENT_NUM %>
    <%= f.input :RATE_TASK %>
    <%= f.input :RATE_KEY %>
    <%= f.input :RATE_NOTE %>
    <%= f.input :SPECIAL_RATE %>
    <%= f.input :RATE_ADDED_BY %>
    <%= f.input :RATE_ADDED_WHEN %>
    <%= f.input :RATE_EDITED_BY %>
    <%= f.input :RATE_EDITED_WHEN %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
