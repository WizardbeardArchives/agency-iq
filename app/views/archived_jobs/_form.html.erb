<%= simple_form_for(@archived_job) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :AJ_JOB_NUM %>
    <%= f.input :AJ_NAME %>
    <%= f.input :AJ_FORM %>
    <%= f.input :AJ_PRINTER %>
    <%= f.input :AJ_ARTIST %>
    <%= f.input :AJ_LOCATION %>
    <%= f.input :AJ_REF %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
