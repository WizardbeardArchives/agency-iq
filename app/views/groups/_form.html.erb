<%= simple_form_for(@group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :GROUP_NUM %>
    <%= f.input :GROUP_DESC %>
    <%= f.input :GROUP_CAT %>
    <%= f.input :GROUP_ADDED_BY %>
    <%= f.input :GROUP_ADDED_WHEN %>
    <%= f.input :GROUP_EDITED_BY %>
    <%= f.input :GROUP_EDITED_WHEN %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
