<%= simple_form_for(@palm_task) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :PALM_TASK_CODE %>
    <%= f.input :PALM_TASK_NAME %>
    <%= f.input :PALM_TASK_USER %>
    <%= f.input :PALM_TASK_ID %>
    <%= f.input :PALM_TASK_CAT %>
    <%= f.input :PALM_TASK_ATTRIBS %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
