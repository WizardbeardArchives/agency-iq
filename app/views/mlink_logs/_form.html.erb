<%= simple_form_for(@mlink_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :primaryKey %>
    <%= f.input :MLOG_DATE %>
    <%= f.input :MLOG_BUYID %>
    <%= f.input :MLOG_STATUS %>
    <%= f.input :MLOG_ADDED_BY %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
