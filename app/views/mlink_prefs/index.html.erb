        <td><%= mlink_pref.MLINK_AFTER_PREF %></td>
        <td><%= mlink_pref.MLINK_MOVE_PATH %></td>
        <td><%= mlink_pref.MLINK_SYSTEM %></td>
        <td><%= mlink_pref.MLINK_STATUS %></td>
        <td><%= mlink_pref.MLINK_LAST_IMPORT %></td>
        <td><%= mlink_pref.MLINK_COUNT_TODAY %></td>
        <td><%= mlink_pref.MLINK_COUNT_TOTAL %></td>
        <td><%= mlink_pref.MLINK_AMT %></td>
        <td><%= mlink_pref.MLINK_IMPORT_INTERVAL %></td>
        <td><%= mlink_pref.MLINK_DATE %></td>
