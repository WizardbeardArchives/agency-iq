        <td><%= mlink_pref.MLINK_PRINT_BILL_NET %></td>
        <td><%= mlink_pref.MLINK_BROAD_COMM %></td>
        <td><%= mlink_pref.MLINK_PRINT_COMM %></td>
        <td><%= mlink_pref.MLINK_EDITED_BY %></td>
        <td><%= mlink_pref.MLINK_EDITED_WHEN %></td>
        <td><%= link_to 'Show', mlink_pref %></td>
        <td><%= link_to 'Edit', edit_mlink_pref_path(mlink_pref) %></td>
        <td><%= link_to 'Destroy', mlink_pref, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

