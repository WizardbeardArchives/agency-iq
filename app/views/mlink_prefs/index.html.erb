      <th>Mlink print comm</th>
      <th>Mlink edited by</th>
      <th>Mlink edited when</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @mlink_prefs.each do |mlink_pref| %>
      <tr>
        <td><%= mlink_pref.primaryKey %></td>
        <td><%= mlink_pref.MLINK_ENABLE_LOG %></td>
        <td><%= mlink_pref.MLINK_PATH %></td>
        <td><%= mlink_pref.MLINK_AUTO_IMPORT %></td>
        <td><%= mlink_pref.MLINK_IMPORT_EVERY %></td>
        <td><%= mlink_pref.MLINK_IMPORT_TIME %></td>
        <td><%= mlink_pref.MLINK_IMPORT_MINUTES %></td>
